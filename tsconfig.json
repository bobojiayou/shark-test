{
  "compilerOptions": {
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "outDir": "dist",
    "rootDir": ".",
    "skipLibCheck": true,
    "sourceMap": false,
    "target": "es6",
    "lib": [
      "es2017"
    ],
    "baseUrl": "",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "types": [
      "jasmine",
      "node"
    ],
    "paths": {}
  },
  "exclude": [
    "dist/**/*",
    "node_modules/**/*",
    "tmp/**/*",
    "src/initFile/**/*"
  ]
}